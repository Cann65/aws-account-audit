    log().info(</span><span><span class="hljs-string">"Findings: %s | Worst: %s"</span></span><span>, report.summary()[</span><span><span class="hljs-string">"total"</span></span><span>], report.summary()[</span><span><span class="hljs-string">"worst"</span></span><span>])

